<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.chapter6.mapper.FileMapMapper">

<!--    file map 저장-->
    <insert id="insertFileMap" parameterType="FileMapVO">
        insert into file_map(
            board_id,
            file_id
        ) values(
            #{boardId},
            #{fileId}
        )
    </insert>
</mapper>